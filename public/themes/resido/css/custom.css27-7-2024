}
.home__blog .post-date {
    font-size: 14px;
}
.home__blog .blog-thumb img {
    height: auto;
    min-height: 200px;
    object-fit: cover;
}
.sec-heading h2::after{
    content: none!important;    
    position: absolute;
    width: auto;
    height: 5px;
    top: 0;
    margin: auto;
    display: block;
    left: 0;
    right: 0;
    border-radius: 5px;
    background: var(--main-color)!important;
    max-width: 250px;
    margin-left: 0;
}

