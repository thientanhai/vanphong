.swiper-container {
    margin-left: auto;
    margin-right: auto;
    position: relative;
    overflow: hidden;
    z-index: 1;
}


.noselect * {
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    cursor: default;
}

.swiper-container.videoswiper {
    padding: 10px;
    margin-left: -10px;
    margin-right: -10px;
    box-sizing: border-box;
}

.swiper-button-next:after, .swiper-button-prev:after {
    content:""!important;
}
  
.swiper-mobile-app .swiper-button-prev,
.swiper-mobile-app .swiper-button-next {
  height: 40px;
  width: 40px;
  top: 29px;
  background-color:white;
  background-size: 12px;
  border-radius: 50%;
  box-shadow:unset;
  transition: 0.6s all;
  cursor: pointer;
  border: 1px solid #bbbbbb;
}

.swiper-mobile-app .swiper-button-prev:hover,
.swiper-mobile-app .swiper-button-next:hover {
  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.3);
}

.swiper-button-next, .swiper-button-prev {
    position: absolute;
    top: 50%;
    width: 27px;
    height: 44px;
    margin-top: -22px!important ;
    z-index: 0;
    cursor: pointer;
    -moz-background-size: 27px 44px;
    -webkit-background-size: 27px 44px;
    background-size: 27px 44px;
    background-position: center;
    background-repeat: no-repeat;
}

.swiper-mobile-app .swiper-button-prev {
  left: initial;
  right: 60px;
  background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 27 44'%3E%3Cpath d='M0,22L22,0l2.1,2.1L4.2,22l19.9,19.9L22,44L0,22L0,22L0,22z' fill='%23000'/%3E%3C/svg%3E");
}

.swiper-mobile-app .swiper-button-next {
    background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 27 44'%3E%3Cpath d='M27,22L27,22L5,44l-2.1-2.1L22.8,22L2.9,2.1L5,0L27,22L27,22z' fill='%23000'/%3E%3C/svg%3E");
}

.swiper-mobile-app .slick-cloned {
    display: none!important;
}

