.paraBanner__slide__index {
    font-weight: 700;
    font-size: 36px;
    letter-spacing: .0015em;
    text-transform: uppercase;
    color: #fff;
    position: absolute;
    z-index: 2;
    top: calc(50% - 62px);
    left: 3%
}

@media(min-width: 1200px) {
    .paraBanner__slide__index {
        line-height: 44px
    }
}

@media(max-width: 991.98px) {
    .paraBanner__slide__index {
        font-size: 22px;
        top: calc(50% - 45px)
    }
}

.paraBanner__slide__index span {
    display: block
}

.paraBanner__slide__index span.current {
    position: relative
}

@media(min-width: 992px) {
    .paraBanner__slide__index span.current {
        margin-bottom: 40px
    }
}

.paraBanner__slide__index span.current:before {
    content: "";
    position: absolute;
    bottom: -20px;
    left: -22px;
    height: 2px;
    width: 72px;
    background-color: #fff
}

@media(max-width: 991.98px) {
    .paraBanner__slide__index span.current:before {
        bottom: 0;
        left: -8px;
        height: 2px;
        width: 27px
    }
}

.paraBanner__slide__arrow {
    position: absolute;
    z-index: 2;
    top: calc(50% - 62px);
    right: 3%
}

@media(max-width: 991.98px) {
    .paraBanner__slide__arrow {
        top: calc(50% - 45px)
    }
}

.paraBanner__slide__arrow--prev,    
.paraBanner__slide__arrow--next {
    width: 32px;
    height: 32px;
    cursor: pointer;
    /* background: transparent url("https://c3spacedemo.topdealhot.com/wp-content/uploads/2023/09/arrow_white.png") no-repeat center/100% auto */
}

.paraBanner__slide__arrow--prev,
.paraBanner__slide__arrow--next {
    transform: rotate(90deg);
}

.paraBanner__slide__arrow--prev,
.paraBanner__slide__arrow--prev {
    transform: rotate(270deg);
}

@media(max-width: 991.98px) {

    .paraBanner__slide__arrow--prev,
    .paraBanner__slide__arrow--next {
        width: 20px;
        height: 20px
    }
}

.paraBanner__slide__arrow--next {
    transform: rotate(90deg);
    margin-top: 40px;
    position: relative
}

@media(max-width: 991.98px) {
    .paraBanner__slide__arrow--next {
        margin-top: 15px
    }
}

.paraBanner__slide__arrow__custom__hr_next:before {
    content: "";
    position: absolute;
    top: 50px;
    left: -22px;
    height: 1px;
    width: 72px;
    background-color: #fff
}

@media(max-width: 991.98px) {
    .paraBanner__slide__arrow__custom__hr_next:before {
        top: 27px;
        left: -4px;
        height: 1px;
        width: 27px
    }
}

.paraBanner__slide__image {
    overflow: hidden;
    transition: .4s ease;
    position: relative
}

.paraBanner__slide__image:before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color:transparent;
    pointer-events: none;
    z-index: 1;
    border: 1px solid #fff;
    margin: 40px;
    border-radius: 10px;
}

@media(max-width: 1199.98px) {
    .paraBanner__slide__image {
        height: 575px
    }
}

@media(max-width: 991.98px) {
    .paraBanner__slide__image {
        height: 431px
    }
}

@media(max-width: 767.98px) {
    .paraBanner__slide__image {
        height: auto
    }
  
}

@media(max-width: 575.98px) {
    .paraBanner__slide__image {
        height: auto
    }
   
}

.paraBanner__slide__image img {
    transition: all 20s ease;
    transform: scale(1.5);
    width: 100%;
    height: 100%;
    object-fit: cover;
    min-height:400px!important;
}

@media(min-width: 1200px) {
    .paraBanner__slide div {
        height: auto;
        border-radius: 10px;
        /* width: 100%!important; */
    }
}

.paraBanner__slide .slick-list {
    height: 100%
}

.paraBanner__slide .slick-current .paraBanner__slide__image img {
    transform: scale(1)
}

.paraBanner__slide .slick-current .paraBanner__mask__content p {
    height: 100% !important
}

