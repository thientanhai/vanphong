.sec-heading h2 {
    padding-top: 1rem;
    position: relative;
    color: #000;
    font-family: 'Inter', sans-serif;
    font-weight: 600;
}


.sec-heading h2::before {
    content: none!important;
    position: absolute;
    width: 100%;
    height: 2px;
    top: 1px;
    margin: auto;
    display: block;
    left: 0;
    right: 0;
    background: #bbbbbb;
}    
.blog-wrap-grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
}

.home__blog .bl-title {
    font-size: 16px;
    margin-bottom: 10px;
    line-height: 1.5;
